{
  "name": "turing",
  "version": "1.0.0",
  "description": "Turing game",
  "scripts": {
    "start": "parcel start src/index.html --public-url dist",
    "build": "parcel build src/index.html --public-url dist",
    "watch": "parcel watch src/index.html --public-url dist"
  },
  "engines": {
    "node": "12.x"
  },
  "author": "Deniz Basgoren",
  "license": "GPL3",
  "dependencies": {
    "immer": "^5.1.0",
    "marked": "^0.8.0",
    "preact": "^10.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-transform-react-jsx": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "parcel-bundler": "^1.12.4"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      [
        "@babel/plugin-transform-react-jsx",
        {
          "pragma": "h",
          "pragmaFrag": "Fragment"
        }
      ]
    ]
  }
}
